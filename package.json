{
	"name": "@titanium/imagefactory",
	"version": "1.0.12",
	"description": "Axway Amplify module for image transformation - scale, crop, and compress images - for Appcelerator Titanium Mobile",
	"main": "index.js",
	"scripts": {
		"list-files": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
		"postinstall": "tiapp-install-module",
		"postuninstall": "tiapp-uninstall-module",
		"test": "npm install --ignore-scripts && npm run list-files",
		"prepublishOnly": "git pull && bump insane"
	},
	"author": "Axway",
	"maintainers": [
		{
			"name": "Brenton House",
			"email": "brenton.house@gmail.com",
			"url": "https://brenton.house"
		}
	],
	"repository": {
		"type": "git",
		"url": "github:brentonhouse/titanium-imagefactory"
	},
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/brentonhouse/titanium-imagefactory/issues"
	},
	"homepage": "https://brenton.house/saying-goodbye-to-axway-amplify-titanium-31a44f3671de",
	"keywords": [
		"titanium",
		"mobile",
		"alloy",
		"turbo",
		"axway",
		"appcelerator",
		"ios",
		"android",
		"iphone",
		"ipad",
		"titanium-native-module",
		"sdk",
		"plugin",
		"amplify",
		"api-builder",
		"appcelerator-titanium",
		"titanium-sdk",
		"titanium-mobile",
		"titanium-module",
		"axway-sdk",
		"axway-amplify",
		"brenton-house"
	],
	"titanium": {
		"type": "native-module",
		"platform": {
			"ios": {
				"moduleid": "ti.imagefactory",
				"name": "imagefactory",
				"version": "1.3.0",
				"apiversion": 2,
				"architectures": [
					"armv7",
					"arm64",
					"i386",
					"x86_64"
				],
				"minsdk": "5.0.0",
				"guid": "0aab25d7-0486-40ab-94a3-ed4f9a293414"
			},
			"android": {
				"moduleid": "ti.imagefactory",
				"name": "imagefactory",
				"version": "4.0.0",
				"apiversion": 4,
				"architectures": [
					"arm64-v8a",
					"armeabi-v7a",
					"x86"
				],
				"minsdk": "7.0.0",
				"guid": "0aab25d7-0486-40ab-94a3-ed4f9a293414"
			}
		}
	},
	"dependencies": {
		"@titanium/tiapp-xml": "^1.0.4"
	},
	"devDependencies": {
		"@titanium/essentials": "^1.0.30",
		"lodash": "^4.17.15"
	}
}
